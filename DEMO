/* Animated Bulb Container */
.bulb-container {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 35px;
    width: 20px;
}

/* Bulb Wire Animation */
.bulb-wire {
    position: absolute;
    top: -15px;
    left: 50%;
    transform: translateX(-50%);
    width: 2px;
    height: 20px;
    background: linear-gradient(to bottom, #666, #999);
    border-radius: 1px;
    transform-origin: top center;
    transition: all 0.4s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

/* Bulb Head */
.bulb-head {
    position: relative;
    top: 5px;
    transform-origin: top center;
    transition: all 0.4s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

/* Swing Animation on Click */
.theme__toggle.clicked .bulb-wire {
    animation: wireSwing 0.8s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

.theme__toggle.clicked .bulb-head {
    animation: bulbSwing 0.8s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

/* Keyframes for Wire Swing */
@keyframes wireSwing {
    0% { transform: translateX(-50%) rotate(0deg); }
    25% { transform: translateX(-50%) rotate(15deg); }
    50% { transform: translateX(-50%) rotate(-10deg); }
    75% { transform: translateX(-50%) rotate(5deg); }
    100% { transform: translateX(-50%) rotate(0deg); }
}

/* Keyframes for Bulb Head Swing */
@keyframes bulbSwing {
    0% { transform: rotate(0deg); }
    25% { transform: rotate(15deg); }
    50% { transform: rotate(-10deg); }
    75% { transform: rotate(5deg); }
    100% { transform: rotate(0deg); }
}

/* Theme-based Bulb Glow */
:root[data-theme="default"] .bulb-head i {
    color: #fff;
    text-shadow: 0 0 10px rgba(255, 255, 255, 0.3);
    transition: all 0.3s ease;
}

:root[data-theme="light"] .bulb-head i {
    color: #000;
    text-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
    transition: all 0.3s ease;
}

/* Wire Color Changes Based on Theme */
:root[data-theme="default"] .bulb-wire {
    background: linear-gradient(to bottom, #666, #999);
}

:root[data-theme="light"] .bulb-wire {
    background: linear-gradient(to bottom, #8B4513, #A0522D);
}

/* Hover Effects */
.theme__toggle:hover .bulb-head i {
    transform: scale(1.1);
