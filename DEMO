        <script>
            // Only show on homepage
            if (window.location.pathname === &#39; /&#39; || window.location.pathname === &#39;/index.html &#39;) {
                document.querySelectorAll(&#39;.tag - item &#39;).forEach(tag =&gt; {
                    const labelName = tag.getAttribute(&#39; href &#39;).split(&#39;/&#39;).pop();

                    // JSON-P feed URL (Blogger-friendly)
                    const feedUrl = `/feeds/posts/default/-/${labelName}?alt=json-in-script&amp;max-results=1`;

                    const script = document.createElement(&#39; script &#39;);
                    const callbackName = &#39; cb_ &#39; + labelName.replace(/[^a-zA-Z0-9]/g,&#39; _ &#39;);

                    window[callbackName] = function (data) {
                        const totalPosts = data.feed.openSearch$totalResults.$t || 0;
                        if (totalPosts &gt; 0) {
                            tag.textContent += ` (${totalPosts})`;
                        }
                        document.body.removeChild(script);
                    };

                    script.src = feedUrl + `&amp;callback=${callbackName}`;
                    document.body.appendChild(script);
                });
            } else {
                // Hide the tag container on subpages
                const container = document.querySelector(&#39;.tag - container &#39;);
                if (container) container.style.display = &#39; none &#39;;
            }
        </script>



