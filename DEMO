       <script>
const seasons = [
  {name: &quot;spring&quot;, label: &quot;Spring&quot;},
  {name: &quot;summer&quot;, label: &quot;Summer&quot;},
  {name: &quot;fall&quot;, label: &quot;Fall&quot;},
  {name: &quot;winter&quot;, label: &quot;Winter&quot;}
];

seasons.forEach(season =&gt; {
  const box = document.querySelector(`.calendar-box.${season.name}`);
  const span = box.querySelector(&#39;span&#39;);
  const feedUrl = `/feeds/posts/default/-/${season.label}?alt=json&amp;max-results=0`; // correct &amp;

  fetch(feedUrl)
    .then(res =&gt; res.json())
    .then(data =&gt; {
      const count = data.feed.openSearch$totalResults.$t;
      if(count &amp;&amp; count &gt; 0) {
        span.textContent += ` (${count})`; // append count
      }
    })
    .catch(err =&gt; console.log(&quot;Error fetching label feed:&quot;, err));
});
</script>



