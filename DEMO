/* Auto-detect and style movie badges */
<script>
    document.addEventListener('DOMContentLoaded', function () {
        const seasonBadges = document.querySelectorAll('.homepage-card__season');
        seasonBadges.forEach(function (badge) {
            const text = badge.textContent.toLowerCase();
            if (text.includes('movie') || text.includes('film')) {
                badge.classList.add('movie-badge');
            }
        });
    });
</script>
