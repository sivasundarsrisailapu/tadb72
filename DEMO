            <script>
// Inline script runs immediately after HTML is rendered
(function() {
  // Wait for DOM to be ready
  function initViewCounters() {
    if (typeof firebase === &#39;undefined&#39; || !firebase.database) {
      setTimeout(initViewCounters, 100);
      return;
    }
    
    const db = firebase.database();
    const isHomepage = window.location.pathname === &#39;/&#39;;
    const mainCounter = document.getElementById(&#39;homepageViews&#39;);
    const totalPostCounter = document.getElementById(&#39;allPostsViews&#39;);
    
    function setupTotalViews(ref, counterElement) {
      if (!ref || !counterElement) return;
      ref.transaction((current) =&gt; (current || 0) + 1);
      ref.on(&#39;value&#39;, (snap) =&gt; {
        const count = snap.val() || 0;
        counterElement.textContent = count.toLocaleString();
      });
      setInterval(() =&gt; {
        ref.once(&#39;value&#39;).then((snap) =&gt; {
          const count = snap.val() || 0;
          counterElement.textContent = count.toLocaleString();
        });
      }, 5000);
    }
    
    if (isHomepage) {
      if (mainCounter) {
        const homeRef = db.ref(&#39;total_views/homepage&#39;);
        setupTotalViews(homeRef, mainCounter);
      }
      if (totalPostCounter) {
        const postsRef = db.ref(&#39;total_views/posts&#39;);
        let totalPostViews = 0;
        postsRef.on(&#39;value&#39;, (snap) =&gt; {
          totalPostViews = 0;
          if (snap.exists()) {
            snap.forEach((child) =&gt; {
              totalPostViews += (child.val() || 0);
            });
          }
          totalPostCounter.textContent = totalPostViews.toLocaleString();
        });
        setInterval(() =&gt; {
          postsRef.once(&#39;value&#39;).then((snap) =&gt; {
            let sum = 0;
            if (snap.exists()) {
              snap.forEach((child) =&gt; {
                sum += (child.val() || 0);
              });
            }
            totalPostCounter.textContent = sum.toLocaleString();
          });
        }, 5000);
      }
    }
  }
  
  initViewCounters();
})();
</script>
        </b:if>


        <b:if cond='data:view.isPost'>
            <script type='text/javascript'>
                //<![CDATA[
                (function () {
                    function initPostCounter() {
                        if (typeof firebase === 'undefined' || !firebase.database) {
                            setTimeout(initPostCounter, 100);
                            return;
                        }

                        const db = firebase.database();
                        const postCounter = document.getElementById('postViews');

                        if (postCounter) {
                            const postCounterSpan = postCounter.closest('span[data-post-id]');
                            const postId = postCounterSpan ? postCounterSpan.dataset.postId : null;

                            if (postId) {
                                const postRef = db.ref('total_views/posts/' + postId);
                                postRef.transaction((current) => (current || 0) + 1);
                                postRef.on('value', (snap) => {
                                    const count = snap.val() || 0;
                                    postCounter.textContent = count.toLocaleString();
                                });
                                setInterval(() => {
                                    postRef.once('value').then((snap) => {
                                        const count = snap.val() || 0;
                                        postCounter.textContent = count.toLocaleString();
                                    });
                                }, 5000);
                            }
                        }
                    }

                    initPostCounter();

                    // More Details Toggle Function
                    window.toggleMoreDetails = function () {
                        const panel = document.getElementById('moreDetailsPanel');
                        const button = document.querySelector('.btn-more-details');

                        if (panel && button) {
                            if (panel.style.display === 'none' || panel.style.display === '') {
                                panel.style.display = 'block';
                                button.classList.add('active');
                                button.innerHTML = button.innerHTML.replace('View More Details', 'Hide Details');
                            } else {
                                panel.style.display = 'none';
                                button.classList.remove('active');
                                button.innerHTML = button.innerHTML.replace('Hide Details', 'View More Details');
                            }
                        }
                    };

                    // Episode Count Placeholder
                    function updateEpisodeCount() {
                        const episodeCountElement = document.getElementById('episodeCountNumber');
                        if (episodeCountElement) {
                            episodeCountElement.textContent = '--';
                        }
                    }
                    document.addEventListener('DOMContentLoaded', updateEpisodeCount);

                    // Firebase Rating System
                    function initRatingSystem() {
                        if (typeof firebase === 'undefined' || !firebase.database) {
                            setTimeout(initRatingSystem, 100);
                            return;
                        }

                        const db = firebase.database();
                        const postIdEl = document.querySelector('.rating-section[data-post-id]');
                        if (!postIdEl) {
                            console.log('Rating section not found');
                            return;
                        }
                        const postId = postIdEl.getAttribute('data-post-id');
                        if (!postId) {
                            console.log('Post ID not found');
                            return;
                        }

                        console.log('Initializing rating system for post:', postId);

                        // Test Firebase connection first
                        db.ref('.info/connected').on('value', (snapshot) => {
                            if (snapshot.val() === true) {
                                console.log('Firebase connected successfully');
                            } else {
                                console.log('Firebase disconnected');
                                showMessage('Connection lost. Please check your internet.', 'error');
                            }
                        });

                        const ratingRef = db.ref('post_ratings/' + postId);
                        const stars = document.querySelectorAll('.star');
                        const currentRatingEl = document.getElementById('currentRating');
                        const voteCountEl = document.getElementById('voteCount');
                        const messageEl = document.getElementById('ratingMessage');

                        let userRating = 0;
                        let hasVoted = localStorage.getItem('voted_' + postId) === 'true';

                        // Load and sync rating data
                        ratingRef.on('value', (snapshot) => {
                            const data = snapshot.val();
                            let averageRating = 0;
                            let totalVotes = 0;

                            if (data) {
                                const totalRating = data.totalRating || 0;
                                totalVotes = data.totalVotes || 0;
                                averageRating = totalVotes > 0 ? (totalRating / totalVotes) : 0;
                            }

                            // Update rating section display
                            if (currentRatingEl) currentRatingEl.textContent = averageRating.toFixed(1);
                            if (voteCountEl) voteCountEl.textContent = totalVotes;

                            // Update main rating display at top
                            const mainRatingEl = document.querySelector('.rating-value');
                            const mainVotesEl = document.querySelector('.rating-votes');
                            if (mainRatingEl) mainRatingEl.textContent = averageRating.toFixed(1);
                            if (mainVotesEl) mainVotesEl.textContent = '(' + totalVotes + ' votes)';

                            console.log('Rating updated:', averageRating.toFixed(1), 'votes:', totalVotes);
                        }, (error) => {
                            console.error('Firebase connection error:', error);
                            showMessage('Unable to load ratings. Please refresh the page.', 'error');
                        });

                        stars.forEach((star, index) => {
                            star.addEventListener('mouseenter', () => {
                                if (!hasVoted) highlightStars(index + 1);
                            });
                            star.addEventListener('mouseleave', () => {
                                if (!hasVoted) highlightStars(userRating);
                            });
                            star.addEventListener('click', () => {
                                if (!hasVoted) {
                                    userRating = index + 1;
                                    submitRating(userRating);
                                } else {
                                    showMessage('You have already voted for this anime!', 'info');
                                }
                            });
                        });

                        function highlightStars(rating) {
                            stars.forEach((star, index) => {
                                star.classList.remove('selected', 'hover');
                                if (index < rating) {
                                    star.classList.add(hasVoted ? 'selected' : 'hover');
                                }
                            });
                        }

                        function submitRating(rating) {
                            showMessage('Submitting your rating...', 'info');

                            // First check if we're connected
                            db.ref('.info/connected').once('value').then((snapshot) => {
                                if (!snapshot.val()) {
                                    throw new Error('No internet connection');
                                }

                                // Proceed with rating submission
                                return ratingRef.transaction((currentData) => {
                                    if (currentData === null) {
                                        return {
                                            totalRating: rating,
                                            totalVotes: 1,
                                            lastUpdated: firebase.database.ServerValue.TIMESTAMP
                                        };
                                    } else {
                                        return {
                                            totalRating: (currentData.totalRating || 0) + rating,
                                            totalVotes: (currentData.totalVotes || 0) + 1,
                                            lastUpdated: firebase.database.ServerValue.TIMESTAMP
                                        };
                                    }
                                });
                            }).then(() => {
                                hasVoted = true;
                                localStorage.setItem('voted_' + postId, 'true');
                                localStorage.setItem('rating_' + postId, rating);
                                highlightStars(rating);
                                showMessage('Thank you for rating! ', 'success');
                                stars.forEach(star => star.style.cursor = 'default');
                            }).catch((error) => {
                                console.error('Rating submission failed:', error);
                                let errorMessage = 'Failed to submit rating. ';

                                if (error.code === 'PERMISSION_DENIED') {
                                    errorMessage += 'Database access denied. Please contact admin.';
                                } else if (error.message.includes('connection') || error.message.includes('network')) {
                                    errorMessage += 'Please check your internet connection.';
                                } else {
                                    errorMessage += 'Please try again later.';
                                }

                                showMessage(errorMessage, 'error');
                                userRating = 0;
                                highlightStars(0);
                            });
                        }

                        function showMessage(text, type) {
                            messageEl.textContent = text;
                            messageEl.className = 'rating-message ' + type;
                            setTimeout(() => {
                                messageEl.textContent = '';
                                messageEl.className = 'rating-message';
                            }, 3000);
                        }

                        if (hasVoted) {
                            const savedRating = localStorage.getItem('rating_' + postId);
                            if (savedRating) {
                                userRating = parseInt(savedRating);
                                highlightStars(userRating);
                            }
                            stars.forEach(star => star.style.cursor = 'default');
                            showMessage('You have already rated this anime', 'info');
                        } else {
                            showMessage('Click stars to rate this anime', 'info');
                        }
                    }

                    initRatingSystem();
                })();
                //]]>
            </script>
        </b:if>








