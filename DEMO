                                <script>
var trailerEmbed = &quot;<b:eval expr='data:movie.trailer'/>&quot;;

// Trailer functionality
document.addEventListener(&#39;DOMContentLoaded&#39;, function() {
  // Count episodes and update display
  var episodeCards = document.querySelectorAll(&#39;.episode-card&#39;);
  var episodeCountElement = document.getElementById(&#39;episodeCount&#39;);
  
  if (episodeCountElement &amp;&amp; episodeCards.length &gt; 0) {
    episodeCountElement.textContent = episodeCards.length;
  }
  
  // Trailer button functionality
  var trailer = document.querySelector(&quot;#trailer&quot;);
  var trailerIframe = document.querySelector(&quot;#trailer iframe&quot;);
  var closeTrailer = document.querySelector(&quot;.modal-video-close-btn&quot;);
  var trailerButton = document.querySelector(&quot;.btn-trailer&quot;);
  
  if (trailerButton &amp;&amp; trailer &amp;&amp; trailerIframe) {
    trailerButton.addEventListener(&quot;click&quot;, function() {
      trailer.style.display = &quot;block&quot;;
      trailerIframe.src = &quot;https://www.youtube.com/embed/&quot; + trailerEmbed + &quot;?autoplay=1&amp;cc_load_policy=1&amp;controls=1&amp;disablekb=0&amp;enablejsapi=0&amp;fs=1&amp;iv_load_policy=1&amp;loop=0&amp;rel=0&amp;showinfo=1&amp;start=0&amp;wmode=transparent&amp;theme=dark&quot;;

    });
  }
  
  if (closeTrailer &amp;&amp; trailer &amp;&amp; trailerIframe) {
    closeTrailer.addEventListener(&quot;click&quot;, function() {
      trailer.style.display = &quot;none&quot;;
      trailerIframe.src = &quot;about:blank&quot;;
    });
  }
});
</script>

