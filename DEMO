    <script type='text/javascript'>
        //<![CDATA[
        // Men
        let menu = document.querySelector(".navigation__toggle"),
            menuIcon = document.querySelector(".navigation__toggle i");

        menu.addEventListener("click", function () {
            document.body.classList.toggle("is-navigation-open");

            if (document.body.classList.contains("is-navigation-open")) {
                menuIcon.classList.remove("fa-bars");
                menuIcon.classList.add("fa-times");

            } else {
                menuIcon.classList.remove("fa-times");
                menuIcon.classList.add("fa-bars");
            }

        });

        // Buscar
        let searchToggle = document.querySelector(".search__toggle.button--icon"),
            searchToggleIcon = document.querySelector(".search__toggle.button--icon i");

        searchToggle.addEventListener("click", function () {
            document.body.classList.toggle("is-search-open");

            if (document.body.classList.contains("is-search-open")) {
                searchToggleIcon.classList.remove("fa-search");
                searchToggleIcon.classList.add("fa-times");

            } else {
                searchToggleIcon.classList.remove("fa-times");
                searchToggleIcon.classList.add("fa-search");
            }

        });
        //]]>
    </script>

    <!-- Theme Toggle Script -->
    <script type='text/javascript'>
        //<![CDATA[
        // Theme toggle functionality
        function toggleTheme() {
            const html = document.documentElement;
            const currentTheme = html.getAttribute('data-theme');
            const newTheme = currentTheme === 'light' ? 'default' : 'light';

            // Add animation class to bulb
            const themeToggle = document.querySelector('.theme__toggle');
            themeToggle.classList.add('clicked');

            // Remove animation class after animation completes
            setTimeout(() => {
                themeToggle.classList.remove('clicked');
            }, 800);

            html.setAttribute('data-theme', newTheme);
            localStorage.setItem('theme', newTheme);
            localStorage.setItem('themeOverride', 'true'); // Mark that user manually changed theme
        }

        // Auto-detect system theme preference
        function getSystemTheme() {
            if (window.matchMedia && window.matchMedia('(prefers-color-scheme: light)').matches) {
                return 'light';
            }
            return 'default'; // Default to dark theme
        }

        // Load theme on page load
        document.addEventListener('DOMContentLoaded', function () {
            const themeOverride = localStorage.getItem('themeOverride');
            let theme;

            if (themeOverride === 'true') {
                // User has manually set a theme, use saved preference
                theme = localStorage.getItem('theme') || 'default';
            } else {
                // Auto-detect based on system preference
                theme = getSystemTheme();
            }

            document.documentElement.setAttribute('data-theme', theme);

            // Add Telugu language tag styling
            const languageTags = document.querySelectorAll('.language-tag');
            languageTags.forEach(function (tag) {
                if (tag.textContent.trim().toLowerCase().includes('telugu')) {
                    tag.classList.add('telugu-lang');
                }
            });
        });

        // Listen for system theme changes (when user changes system theme)
        if (window.matchMedia) {
            window.matchMedia('(prefers-color-scheme: light)').addEventListener('change', function (e) {
                const themeOverride = localStorage.getItem('themeOverride');

                // Only auto-change if user hasn't manually set a theme
                if (themeOverride !== 'true') {
                    const newTheme = e.matches ? 'light' : 'default';
                    document.documentElement.setAttribute('data-theme', newTheme);
                }
            });
        }
        //]]>
    </script>

